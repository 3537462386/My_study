/* pages/serch/serch.wxss */
.container {
    margin: 0;
    padding: 0;
    height: 100vh;
    display: flex;
    flex-direction: column;
    background-color: #F5F5F5;

}

.group image {
    width: 80rpx;
    height: 80rpx;
}

.group {

    background-color: white;
    margin: 10rpx 0;
}

.item {

    display: flex;
    flex-direction: row;
    border: 0.1rpx solid #F5F5F5;
    padding: 10rpx;
    align-items: center;
}